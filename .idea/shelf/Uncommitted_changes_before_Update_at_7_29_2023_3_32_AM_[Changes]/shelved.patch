  )\r\n}\r\n\r\n\r\nconst getCleanArr = (arr) => {\r\n  let result = [];\r\n\r\n  for (let item of arr){\r\n    if(item !== \"\"){\r\n      result.push(item);\r\n    }\r\n  }\r\n\r\n  return result\r\n}
